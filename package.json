{
  "name": "mui-storyblok",
  "version": "1.0.24",
  "description": "use react and mui to build pages with storyblok",
  "author": "apjames93",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "apjames93/mui-storyblok"
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "import"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ],
          "paths": [
            "src"
          ]
        }
      }
    },
    "env": {
      "jest": true
    },
    "globals": {
      "document": false,
      "window": true
    },
    "rules": {
      "react/destructuring-assignment": [
        0,
        {
          "extensions": [
            ".jsx"
          ]
        }
      ],
      "import/no-named-as-default": [
        0
      ],
      "import/no-extraneous-dependencies": [
        0,
        {
          "devDependencies": true
        }
      ],
      "react/prefer-stateless-function": [
        0,
        {
          "ignorePureComponents": true
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/jsx-tag-spacing": [
        1
      ],
      "jsx-a11y/label-has-for": [
        1
      ],
      "react/jsx-boolean-value": [
        0
      ],
      "react/no-array-index-key": [
        0
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/interceptors.js",
      "!src/utils/SessionStorage.js",
      "!src/utils/GoogleHelpers.js",
      "!src/utils/Storyblok.js",
      "!src/serviceWorker.js",
      "!src/setupTests.js",
      "!src/theme.js",
      "!src/index.js",
      "!src/testUtils/LocalStorageMock.js",
      "!src/styleguide/*"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.5.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "3.0.0-beta.5",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.4.1",
    "react-styleguidist": "^11.0.4",
    "react-test-renderer": "^16.3.2",
    "storyblok-js-client": "^1.0.21"
  },
  "peerDependencies": {
    "react-hot-loader": "3.0.0-beta.5",
    "axios": "^0.18.0",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^3.0.2",
    "node-sass": "^4.13.1",
    "storyblok-js-client": "^1.0.21",
    "react-router-dom": "^4.3.1",
    "prop-types": "^15.5.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-examples": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-storyblok-schema": "NODE_ENV=production babel storyblok --out-dir dist/storyblok --copy-files && NODE_ENV=production babel cli.js --out-file dist/cli.js && chmod +x dist/cli.js",
    "build-app": "NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "build": "rm -rf dist && npm run build-app && npm run build-storyblok-schema",
    "publish-npm": "npm run build && npm publish",
    "test-ci": "react-scripts test --env=jsdom --coverage --runInBand --testPathPattern=src/lib/components",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "deploy-docs": "npm run styleguide:build && aws s3 sync styleguide/ s3://mui-storyblok-docs.com/ --delete",
    "export-storyblok": "npx storyblok-component-export"
  },
  "bin": {
    "migrate-storyblok": "./cli.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "moxios": "^0.4.0",
    "react-swipeable-views": "^0.13.9",
    "storyblok-migrate": "^0.3.3",
    "react-router-dom": "^4.3.1",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^3.0.2",
    "node-sass": "^4.13.1"
  }
}
